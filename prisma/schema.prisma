generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  idx       Int     @id @default(autoincrement())
  id        String  @unique
  password  String
  email     String  @unique
  userPic   String
}

model Bookmakr {
  mark_id   Int     @id @default(autoincrement())
  name      String
  address   String  @unique
}